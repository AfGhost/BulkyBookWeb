    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {              /*Database tilkobling HER. Ved en spesiell blokk som er tilgjengelig i navnet ConnectionString*/
